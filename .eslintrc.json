{
  "parserOptions": {
    "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["github", "react"],
  "extends": ["plugin:github/recommended", "plugin:react/recommended"],
  "rules": {
    "camelcase": "off",
    "no-console": "off"
  },
  "settings": {
    "react": {
      "version": ">=16"
    }
  }
}
